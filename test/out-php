Dialect.VERSION = 0.1

SELECT `t`.`f1` AS `f1`,`t`.`f2` AS `f2`,`t2`.`f3` AS `f3`
FROM `t`
INNER JOIN `t2` ON `t`.`id`=`t2`.`id`
WHERE (`f1` = '2')

SELECT `*`
FROM `table` AS `main`
INNER JOIN `usermeta` AS `usermeta1` ON `main`.`ID`=`usermeta1`.`user_id`
WHERE (`usermeta1`.`meta_key` = 'project') AND (`usermeta1`.`meta_value` IN (1,2,3)) AND (`main`.`name` LIKE %l:name%) AND (`main`.`year` = YEAR(date))
ORDER BY `main`.`field1` ASC,`main`.`field2` ASC
LIMIT 2000,1000

SELECT `*`
FROM `table` AS `main`
INNER JOIN `usermeta` AS `usermeta1` ON `main`.`ID`=`usermeta1`.`user_id`
WHERE (`usermeta1`.`meta_key` = 'project') AND (`usermeta1`.`meta_value` IN (1,2,3)) AND (`main`.`name` LIKE '%na\\%me%') AND (`main`.`year` = YEAR(date))
ORDER BY `main`.`field1` ASC,`main`.`field2` ASC
LIMIT 2000,1000

